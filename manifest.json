{
  "name": "Web Launcher",
  "version": "0.1",
  "description": "Assistive Page Launcher",
  "manifest_version": 2,
  "browser_action": {},
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens launcher.html"
    }
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["launcher.html"]
}
